PImage eyeIris;

void setup() {
  imageMode(CENTER);
  eyeIris = loadImage("eyeIris.png");
  size(1840, 1840);
}


void draw() {
  for (int i=0; i<50; i++) {
    colorMode(HSB, 255);
    color eyeColor = color(random(75, 175), random(100, 255), random(200, 255));
    tint(eyeColor);
    image(eyeIris, width/2, height/2);
    noTint();
    String path="./onlyEyes/eye"+i+".png";
    save(path);
  }
  noLoop();
}
